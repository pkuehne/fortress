{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "/usr/local/include",
                "/usr/include/x86_64-linux-gnu",
                "/usr/include",
                "${workspaceRoot}",
                ".",
                "${workspaceFolder}/**",
                "${workspaceFolder}/third-party/yaml-cpp/include",
                "${workspaceFolder}/third-party/glfw/include",
                "${workspaceFolder}/src",
                "${workspaceFolder}/src/core",
                "${workspaceFolder}/src/components",
                "${workspaceFolder}/src/systems"
            ],
            "defines": [
                "MAJOR=0",
                "MINOR=0",
                "PATCH=0",
                "BUILD=0"
            ],
            "intelliSenseMode": "linux-clang-x64",
            "browse": {
                "path": [
                    "/usr/local/include",
                    "/usr/include/x86_64-linux-gnu",
                    "/usr/include",
                    "${workspaceRoot}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}